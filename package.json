{
  "name": "fpsak-frontend",
  "description": "Klient for FPSAK",
  "main": "./dist/index.js",
  "author": "NAV",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/fpsak-frontend.git"
  },
  "scripts": {
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu mochapack --colors --webpack-config webpack/webpack.test.js \"packages/**/*.spec.*\"",
    "test:watch": "cross-env NODE_ICU_DATA=./node_modules/full-icu mochapack --watch --webpack-config webpack/webpack.test.watch.js \"packages/**/*.spec.*\"",
    "test:coverage": "cross-env NODE_ICU_DATA=./node_modules/full-icu nyc mochapack --webpack-config webpack/webpack.test.coverage.js \"packages/**/*.spec.*\"",
    "dev": "cross-env NODE_ENV=development NODE_ICU_DATA=./node_modules/full-icu node webpack/webpack.dev.server.js --config webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --no-progress --colors --config webpack/webpack.prod.js",
    "lerna": "lerna",
    "lerna:publish": "(lerna updated && lerna publish --cd-version patch --yes -m \"publish [ci skip]\") || echo \"Ingen endringer avslutter OK\"",
    "less:lint": "stylelint \"packages/**/*.less\"",
    "ts-check": "tsc --pretty",
    "up": "docker-compose up -d --remove-orphans --build",
    "down": "docker-compose down",
    "lint": "./node_modules/.bin/eslint ./packages/**/*.jsx",
    "remove-node-modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "lint:config": "node -e 'console.dir(require(\"./.eslintrc.js\"))'",
    "storybook": "start-storybook -p 9001 -c packages/storybook/.storybook",
    "deploy-storybook": "build-storybook -c packages/storybook/.storybook -o .static_storybook && storybook-to-ghpages --existing-output-dir=.static_storybook"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.7.4",
    "@navikt/nap-expandable-panel": "1.0.1",
    "@navikt/nap-autocomplete": "^1.0.2",
    "axios": "^0.19.0",
    "core-js": "^3.4.2",
    "dotenv": "^8.0.0",
    "history": "^4.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-modal": "3.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@commercetools/enzyme-extensions": "^5.0.0",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-centered": "^5.2.6",
    "@storybook/addon-docs": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/cli": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/theming": "^5.2.6",
    "@types/chai": "^4.2.5",
    "@types/classnames": "^2.2.8",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/history": "^4.7.3",
    "@types/mocha": "^7.0.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.13",
    "@types/react-collapse": "^5.0.0",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "body-parser": "*",
    "cache-loader": "^4.1.0",
    "chai": "^4.2.0",
    "circular-dependency-plugin": "^5.0.2",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^6.0.2",
    "css-loader": "^1.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.7.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-lerna": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.15.1",
    "file-loader": "^4.3.0",
    "full-icu": "^1.3.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "ignore-styles": "^5.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^15.2.1",
    "json-loader": "^0.5.7",
    "lerna": "^3.19.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.1",
    "mochapack": "^1.1.11",
    "null-loader": "^3.0.0",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-order": "^3.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "thread-loader": "^2.1.2",
    "typescript": "3.7.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "resolutions": {
    "react-test-renderer": "^16.12.0"
  },
  "nyc": {
    "branches": 40,
    "lines": 60,
    "functions": 50,
    "statements": 50,
    "cache": false,
    "check-coverage": true,
    "require": [
      "@babel/register",
      "./setup/setup.jsx"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "text",
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".jsx",
      ".tsx",
      ".ts",
      ".es6"
    ],
    "exclude": [
      "packages/**/*.spec.jsx",
      "packages/**/*.spec.tsx",
      "packages/**/*.spec.ts"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
