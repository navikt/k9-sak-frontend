{
  "name": "k9-sak-web",
  "description": "Klient for K9",
  "main": "./dist/index.js",
  "author": "NAV",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/k9-sak-web.git"
  },
  "scripts": {
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu mochapack --colors --webpack-config webpack/webpack.test.js \"packages/**/*.spec.*\"",
    "test:watch": "cross-env NODE_ICU_DATA=./node_modules/full-icu mochapack --watch --webpack-config webpack/webpack.test.watch.js \"packages/**/*.spec.*\"",
    "test:coverage": "cross-env NODE_ICU_DATA=./node_modules/full-icu nyc mochapack --webpack-config webpack/webpack.test.coverage.js \"packages/**/*.spec.*\"",
    "dev": "cross-env NODE_ENV=development NODE_ICU_DATA=./node_modules/full-icu APP_URL_K9FORMIDLING=http://localhost:8060/rest/dummy node webpack/webpack.dev.server.js --config webpack/webpack.dev.js",
    "dev-med-formidling": "cross-env NODE_ENV=development NODE_ICU_DATA=./node_modules/full-icu node webpack/webpack.dev.server.js --config webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --no-progress --colors --config webpack/webpack.prod.js",
    "lerna": "lerna",
    "lerna:publish": "(lerna updated && lerna publish --cd-version patch --yes -m \"publish [ci skip]\") || echo \"Ingen endringer avslutter OK\"",
    "less:lint": "stylelint \"packages/**/*.less\"",
    "ts-check": "tsc --pretty",
    "up": "docker-compose up -d --remove-orphans --build",
    "down": "docker-compose down",
    "lint": "./node_modules/.bin/eslint ./packages/**/*.jsx",
    "remove-node-modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "lint:config": "node -e 'console.dir(require(\"./.eslintrc.js\"))'",
    "storybook": "start-storybook -p 9001",
    "deploy-storybook": "build-storybook -o .static_storybook && storybook-to-ghpages --existing-output-dir=.static_storybook"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@navikt/nap-autocomplete": "^1.0.2",
    "@navikt/nap-expandable-panel": "1.0.1",
    "@sentry/browser": "^5.29.1",
    "axios": "^0.19.2",
    "core-js": "^3.8.1",
    "dotenv": "^8.2.0",
    "history": "^4.9.0",
    "node-cache": "^5.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "3.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@commercetools/enzyme-extensions": "^5.0.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@storybook/addon-actions": "^6.1.11",
