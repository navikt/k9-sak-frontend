    "json-loader": "^0.5.7",
    "lerna": "^3.22.1",
    "less": "^3.13.0",
    "less-loader": "^7.1.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.2",
    "mochapack": "^1.1.15",
    "null-loader": "^4.0.1",
    "nyc": "^15.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "sinon": "^9.2.2",
    "sinon-chai": "^3.5.0",
    "storybook-react-router": "^1.0.8",
    "style-loader": "^1.2.1",
    "stylelint": "^13.8.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^4.2.2",
    "thread-loader": "^3.0.1",
    "typescript": "4.1.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.2"
  },
  "resolutions": {
    "react-test-renderer": "^16.13.1",
    "minimist": "^1.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "nyc": {
    "branches": 40,
    "lines": 60,
    "functions": 50,
    "statements": 50,
    "cache": false,
    "check-coverage": true,
    "require": [
      "@babel/register",
      "./setup/setup.jsx"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "text",
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".jsx",
      ".tsx",
      ".ts",
      ".es6"
    ],
    "exclude": [
      "packages/**/*.spec.jsx",
      "packages/**/*.spec.tsx",
      "packages/**/*.spec.ts",
      "packages/storybook/**",
      "packages/utils-test/**"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
