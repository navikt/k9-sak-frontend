    "packages/fp-felles",
    "packages/fakta-omsorgen-for",
    "packages/fakta-opptjening",
    "packages/fakta-uttak",
    "packages/sak-historikk",
    "packages/shared-components",
    "packages/form",
    "packages/tidslinje",
    "packages/types",
    "packages/behandling-pleiepenger",
    "packages/prosess-uttak",
